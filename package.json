{
  "name": "@byahri/utils",
  "version": "1.0.0",
  "description": "utility functions that i use for arii and more",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "files": ["dist"],
  "type": "module",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ByAhri/arii-lavalink-client.git"
  },
  "keywords": [
    "lavalink",
    "music",
    "extras"
  ],
  "author": "ByAhri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ByAhri/arii-lavalink-client/issues"
  },
  "homepage": "https://github.com/ByAhri/arii-lavalink-client#readme",
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.8.3"
  }
}
